#include <bits/stdc++.h>
using namespace std;
void topoSort(int V, vector<vector<int>>& adj) {
    vector<int> indegree(V, 0);
    for (auto& u : adj)
        for (auto v : u) indegree[v]++;
    queue<int> q;
    for (int i = 0; i < V; i++)
        if (indegree[i] == 0) q.push(i);
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";
        for (int v : adj[node])
            if (--indegree[v] == 0) q.push(v);
    }
}
int main() {
    int V = 6;
    vector<vector<int>> adj = {
        {2, 3}, {3, 4}, {}, {5}, {}, {}
    };
    topoSort(V, adj);
}
